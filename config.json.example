{
  "default": {
    "redis_url": "redis://127.0.0.1:6379",
    "key": "remote_addr",
    "rate": 10,
    "burst": 5,
    "algorithm": "sliding_window",
    "window_size": 60,
    "enabled": true
  },
  "locations": {
    "/": {
      "rate": 20,
      "burst": 10,
      "algorithm": "sliding_window",
      "window_size": 60
    },
    "/api": {
      "key": "http_x_api_key",
      "rate": 5,
      "burst": 2,
      "algorithm": "token_bucket",
      "window_size": 120
    },
    "/fixed": {
      "rate": 15,
      "burst": 5,
      "algorithm": "fixed_window",
      "window_size": 60
    },
    "/token": {
      "rate": 10,
      "burst": 20,
      "algorithm": "token_bucket"
    },
    "/leaky": {
      "rate": 5,
      "burst": 10,
      "algorithm": "leaky_bucket"
    },
    "/sliding": {
      "rate": 15,
      "burst": 5,
      "algorithm": "sliding_window",
      "window_size": 30
    },
    "/static": {
      "enabled": false
    }
  }
}
